execute if score #fathoms.essence.aberration fathoms.dummy matches 0 run effect give @s minecraft:luck 300
execute if score #fathoms.essence.aberration fathoms.dummy matches 1 run effect give @s minecraft:luck 450 1
execute if score #fathoms.essence.aberration fathoms.dummy matches 2 run effect give @s minecraft:luck 600 2

execute if score #fathoms.essence.aberration fathoms.dummy matches 2 run advancement grant @s only fathoms:nautical/all_max_level_rituals rite_of_fortune